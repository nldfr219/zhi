/* Begin StudentCharts Top Loader */

#progressBarSpace {
	min-height: 2px;
	width: 946px;
	}
	
#progressBar {
	position: absolute;	
	white-space: nowrap;
	overflow: hidden;	
	background: #e3e3e3;	
   	background: -moz-linear-gradient(left, #000000, #e3e3e3);
    background: -webkit-gradient(linear, left top, right top, from(#000000), to(#e3e3e3));	
	margin: 0 2px 0 2px;
	height: 2px;
	width: 400px;
	animation-name: scProgressBar;
	animation-duration: 3.0s;
	animation-iteration-count: once;
	animation-timing-function: cubic-bezier(0.435, 1.005, 0.595, 0.965); 	
	-moz-animation-name: scProgressBar;
	-moz-animation-duration: 3.0s;
	-moz-animation-iteration-count: once;
	-moz-animation-timing-function: cubic-bezier(0.435, 1.005, 0.595, 0.965); 
	-webkit-animation-name: scProgressBar;
	-webkit-animation-duration: 3.0s;
	-webkit-animation-iteration-count: once;
	-webkit-animation-timing-function: cubic-bezier(0.435, 1.005, 0.595, 0.965); 	
}

@keyframes scProgressBar {
	to {
		-webkit-transform:translateX(546px);
	}	
}

@-moz-keyframes scProgressBar {
	to {
		-moz-transform:translateX(546px);
	}	
}

@-webkit-keyframes scProgressBar {
	to {
		-webkit-transform:translateX(546px);
	}
}

